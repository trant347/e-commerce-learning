{
  "ConsulConfig": {
    "ServiceId": "calendar-service",
    "ServiceName": "calendar-service",
    "ServiceAddress": "http://localhost",
    "ServicePort": 5000,
    "HealthCheckUrl": "http://calendar-service:8080/health",
    "HealthCheckIntervalSeconds": 10,
    "HealthCheckTimeoutSeconds": 5,
    "ConsulAddress": "http://consul:8500"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KafkaProducerConfig": {
    "BootstrapServers": "kafka:29092",
    "Topic": "bookings",
    "ClientId": "calendar-service"
  },
  "AllowedHosts": "*"
}
